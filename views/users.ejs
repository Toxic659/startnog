<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<style media="screen">
body {
  font-family: 'Lato', sans-serif;
  color: #4A4A4A;
  justify-content:center;
  align-items: center;
  margin:0;
  padding: 0;
    background-color: #000201;
    color: white;
}
form{
  width: 80%;

  padding: 30px;
}
.carD{
  margin: 30px auto;

  width: 80%;

  padding: 30px;
  background-color: #6A8498;
}
  .users{
    margin-bottom: 30px;
    margin-top: 30px;
    padding: 30px;
  }
  .sidenavv{
    padding: 10px;
  }
  .sidenavv li{
    margin-bottom: 10px;
  }
  .sidenavv ul li a{
    text-decoration: none;
    color: white;

  }
  .sidenavv a:hover{
    color: green;
  }
  .dashbtn{
    background-color: #e3faac;
    color: black;
    margin-left: 19%;
    margin-top: 10px;
  }
  .dashbtn:hover{
    background-color: green;
  }
</style>
<script src="https://kit.fontawesome.com/ffb4c77547.js" crossorigin="anonymous"></script>

  </head>
  <body>
    <section class="sideBar">
      <a class="btn btn-primary dashbtn" data-bs-toggle="offcanvas" href="#offcanvasDark" role="button" aria-controls="offcanvasDarkLabel"> Dashboard Menu </a>
      <div class="offcanvas offcanvas-start show text-bg-dark" tabindex="-1" id="offcanvasDark" aria-labelledby="offcanvasDarkLabel">
        <div class="offcanvas-header">
          <h5 class="offcanvas-title" id="offcanvasExampleLabel">Starngo</h5>
          <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
          <div class="sidenavv">
            <ul>
              <li><a href="/dash">
                  <i class="fa-solid fa-gauge-high"></i> Main Dashboard </a></li>
              <li> <a href="/cars">
                  <i class="fa-solid fa-car"></i> All Cars </a></li>
              <li> <a href="/rented">
                  <i class="fa-solid fa-car-on"></i>
                  Rented Cars </a></li>
                  <li> <a href="/notrented">
                      <i class="fa-solid fa-car-rear"></i>
                      Avaliable Cars </a></li>
              <li> <a href="/users">
                  <i class="fa-solid fa-users"></i>
                  Users </a></li>
            </ul>
          </div>
        </div>
      </div>
    </section>

  <% users.forEach(function(us){ %>
    <div class="users border border-dark rounded">

    <label for="">The tenant of the car name</label>
    <input class="form-control" type="text" name="renname" value="<%=us.name%>" readonly>
    <label for="">The tenant of the car ID number</label>
    <input class="form-control" type="text" name="renid" value="<%=us.tid%>" readonly>
    <label for="">The beginning of the lease term</label>
    <input class="form-control" type="date" name="rendates" value="<%=us.startDate%>" readonly>
    <label for="">The end of the lease term</label>
    <input class="form-control" type="date" name="rendatee" value="<%=us.endDate%>" readonly>
    <label for="">Car details</label>

    <input class="form-control" type="text" name="rendatee" value="<%=us.carDetails[0].name%>, <%=us.carDetails[0].year%>, <%=us.carDetails[0].model%>, <%=us.carDetails[0].km%> " readonly>

  </div>





  <% }) %>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>

  </body>
</html>
